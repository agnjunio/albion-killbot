{
  "name": "albion-killbot",
  "version": "3.3.9",
  "description": "A Discord bot to display Albion Online kills/deaths in chat.",
  "main": "index.js",
  "repository": "https://github.com/agnjunio/albion-killbot.git",
  "author": "Agnaldo Junior <agnaldo.junior01@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "node ./bin/start.js",
    "start:crawler": "npm start -- --mode crawler",
    "start:crawler:dev": "nodemon -x 'npm run start:crawler'",
    "start:bot": "npm start -- --mode bot",
    "start:bot:dev": "nodemon -x 'npm run start:bot'",
    "start:api": "npm start -- --mode api",
    "start:api:dev": "nodemon -x 'npm run start:api'",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "prettier": "prettier -c .",
    "prettier:fix": "prettier --write ."
  },
  "engines": {
    "node": "16.x",
    "npm": "8.5.x"
  },
  "dependencies": {
    "amqplib": "^0.8.0",
    "aws-sdk": "^2.689.0",
    "axios": "^0.21.2",
    "canvas": "^2.6.1",
    "colors": "^1.4.0",
    "discord.js": "^12.4.1",
    "dotenv": "^8.2.0",
    "i18n": "^0.8.5",
    "jimp": "^0.16.1",
    "minimist": "^1.2.6",
    "moment": "^2.29.2",
    "mongodb": "^3.5.3",
    "winston": "^3.2.1",
    "winston-loggly-bulk": "^3.1.0"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^8.2.0",
    "nodemon": "^2.0.2",
    "prettier": "^2.2.1"
  }
}
