{
  "name": "albion-killbot",
  "version": "4.0.0",
  "description": "A Discord bot to display Albion Online kills/deaths in chat.",
  "main": "index.js",
  "repository": "https://github.com/agnjunio/albion-killbot.git",
  "author": "Agnaldo Junior <agnaldo.junior01@gmail.com>",
  "license": "MIT",
  "bin": {
    "ctl": "./ctl.sh"
  },
  "scripts": {
    "start": "node ./bin/start.js",
    "start:api": "npm start -- --mode api",
    "start:api:dev": "nodemon --config src/interfaces/api/nodemon.json",
    "start:bot": "npm start -- --mode bot",
    "start:bot:dev": "nodemon --config src/interfaces/bot/nodemon.json",
    "start:crawler": "npm start -- --mode crawler",
    "start:crawler:dev": "nodemon --config src/interfaces/crawler/nodemon.json",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "prettier": "prettier -c .",
    "prettier:fix": "prettier --write ."
  },
  "engines": {
    "node": "16.x",
    "npm": "8.5.x"
  },
  "dependencies": {
    "@discordjs/rest": "^0.4.1",
    "amqplib": "^0.9.0",
    "aws-sdk": "^2.1135.0",
    "axios": "^0.27.2",
    "canvas": "^2.6.1",
    "discord-api-types": "^0.33.0",
    "discord.js": "^13.7.0",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "i18n": "^0.14.2",
    "jimp": "^0.16.1",
    "minimist": "^1.2.6",
    "moment": "^2.29.3",
    "mongodb": "^4.6.0",
    "winston": "^3.7.2"
  },
  "devDependencies": {
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "nodemon": "^2.0.16",
    "prettier": "^2.6.2"
  }
}
